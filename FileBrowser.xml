<?xml version="1.0"?>
<Container>
  <Name>FileBrowser</Name>
  <Repository>80x86/filebrowser:2.9.4-amd64</Repository>
  <Registry>https://hub.docker.com/r/80x86/filebrowser</Registry>
  <Support></Support>
  <Project>https://hub.docker.com/r/80x86/filebrowser</Project>
  <Overview>filebrowser</Overview>
  <Description>Use TTYS3 build</Description>
  <Category>Cloud: Network:Web Status:Stable</Category>
  <WebUI>http://[IP]:[PORT:8082]</WebUI>
  <Icon>https://github.com/maschhoff/docker/raw/master/filebrowser/35781395.png</Icon>
  <ExtraParams>--mount type=tmpfs,destination=/tmp --device=/dev/dri/renderD128:/dev/dri/renderD128</ExtraParams>
  <PostArgs></PostArgs>
  <Description></Description>

  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8082</HostPort>
        <ContainerPort>8082</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8082</HostPort>
        <ContainerPort>8082</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Privileged>false</Privileged>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/filebrowser</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>Path that you want manage</HostDir>
      <ContainerDir>/myfiles</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>1000</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>on/off</Value>
      <Name>FB_SSL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>000</Value>
      <Name>UMASK_SET</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>8082</Value>
      <Name>WEB_PORT</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Config Name="Host Port 1" Target="8082" Default="8082" Mode="tcp" Description="Container Port: 8082" Type="Port" Display="always" Required="true" Mask="false">8082</Config>
  <Config Name="Host Port 2" Target="8082" Default="8082" Mode="udp" Description="Container Port: 8082" Type="Port" Display="always" Required="true" Mask="false">8082</Config>

  <Config Name="PUID" Target="PUID" Default="1000" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">1000</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="FB_SSL" Target="FB_SSL" Default="on/off" Mode="" Description="Container Variable: SSL" Type="Variable" Display="always" Required="false" Mask="false">on</Config>
  <Config Name="UMASK_SET" Target="UMASK_SET" Default="000" Mode="" Description="Container Variable: UMASK_SET" Type="Variable" Display="always" Required="false" Mask="false">000</Config>
  <Config Name="WEB_PORT" Target="WEB_PORT" Default="8082" Mode="" Description="Container Variable: WEB_PORT" Type="Variable" Display="always" Required="false" Mask="false">8082</Config>
  
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/FileBrowser" Mode="rw" Description="Container Path: /config" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/FileBrowser</Config>
  <Config Name="Data Path" Target="/myfiles" Default="Path that you want manage" Mode="rw" Description="Container Path: /myfiles" Type="Path" Display="always" Required="true" Mask="false">Path that you want manage</Config>
</Container>
